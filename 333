{
# include <iostream>
    using ramespace std;
    int main() {
        setlocale(0, "");
        cout << "Введите число от 0-100";
        int num;
        cin >> num;
        if (num <= 0 and num <= 10) {
            cout << "Вы попали в диапазон 0 - 10"
        }
        else if (num <= 11 and num <= 20) {
            cout << "Вы попали в диапазон 11-20";
        }
        else (num <= 21 and num <= 30) {
            cout << "Вы попали в диапазон 21-30";
        }
        if (num <= 31 and num <= 40) {
            cout << "Вы попали в диапазон 31-40"
        }

        if (num <= 41 and num <= 50) {
            cout << "Вы попали в диапазон 41-50"
        }

        if (num <= 51 and num <= 60) {
            cout << "Вы попали в диапазон 51-60"
        }

        if (num <= 61 and num <= 70) {
            cout << "Вы попали в диапазон 61-70"
        }
        if (num <= 71 and num <= 80) {
            cout << "Вы попали в диапазон 71-80"
        }
        if (num <= 81 and num <= 90) {
            cout << "Вы попали в диапазон 81-90"
        }
        if (num <= 91 and num <= 100) {
            cout << "Вы попали в диапазон 91-100"
        }
        retufn 0;
    }
